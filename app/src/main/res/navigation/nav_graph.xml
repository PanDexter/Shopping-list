    android:id="@+id/nav_graph"
    app:startDestination="@id/listFragment">
    <fragment
        android:id="@+id/listFragment"
        android:name="com.szeptun.shoppinglist.ui.Lists.ListsFragment"
        android:label="ListFragment">
        <action
            android:id="@+id/action_navigate_to_shoppinglist"
            app:destination="@id/shoppingListFragment" />
    </fragment>
    <fragment
        android:id="@+id/shoppingListFragment"
        android:name="com.szeptun.shoppinglist.ui.ShoppingList.ShoppingListFragment"
        android:label="ShoppingListFragment">
        <argument android:name="items"

            />
    </fragment>
</navigation>